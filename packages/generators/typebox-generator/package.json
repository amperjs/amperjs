{
  "type": "module",
  "name": "@oreum/typebox-generator",
  "version": "0.0.0-alpha.0",
  "cacheVersion": "1",
  "author": "Slee Woo",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
